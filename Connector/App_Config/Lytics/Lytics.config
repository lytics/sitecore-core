<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<pipelines>
			<initialize>
				<processor type="LyticsSitecore.Connector.Pipeline.Initialize.InitializeLytics, LyticsSitecore.Connector" >
					<param name="AccessKey">[Your API KEY]</param>
					<param name="RootAddress">https://api.lytics.io</param>
				</processor>
			</initialize>
		</pipelines>
		<scheduling>
			<agent type="LyticsSitecore.Connector.Agent.RulesRefreshAgent, LyticsSitecore.Connector" method="Run" interval="00:00:15">
			</agent>
		</scheduling>
		</sitecore>
</configuration>